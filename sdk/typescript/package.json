{
  "name": "@agentoven/sdk",
  "version": "0.1.0",
  "description": "AgentOven TypeScript SDK â€” enterprise agent orchestration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": ["agents", "ai", "orchestration", "a2a", "mcp"],
  "repository": {
    "type": "git",
    "url": "https://github.com/agentoven/agentoven"
  },
  "homepage": "https://agentoven.dev",
  "napi": {
    "name": "agentoven",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release ./native",
    "build:debug": "napi build --platform ./native",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "dist",
    "*.node"
  ]
}
